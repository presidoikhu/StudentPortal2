@{
}

@model List<StudentPortal2.Models.Entities.Student>

<table class="table table-dark">
    <thead>
        <tr>

            <th scope="col">ID</th>
            <th scope="col">NAME</th>
            <th scope="col">EMAIL</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var student in Model)
        {
            <tr>

                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>
                    <a asp-action="Edit" asp-controller="Students" asp-route-id="@student.Id">Edit</a>
                </td>

            </tr>
        }



    </tbody>
</table>